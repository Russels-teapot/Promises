{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "no-unused-vars": "off",
    "class-methods-use-this": "off",
    "no-plusplus": "off",
    "no-console": "off"
  },
  "parser": "babel-eslint"
}
